    "df_copy = df.copy()\n",
    "#Make df_copy with changed column names\n",
    "df_copy.rename(columns = {\"Bedrijf\": \"Country Id\", \n",
    "                     \"Dossiernummer\": \"Order ID\",\n",
    "                     \"Vertrekdatum\": \"Departure Date\", \n",
    "                     \"Bedrag\": \"Amount\", \n",
    "                     \"Boekingstype\": \"Category\",\n",
    "                     \"Ticket uitgifte datum\": \"Ticket Issuance Date\", \n",
    "                     \"Luchtvaartmaatschappij\": \"Airline Code\",\n",
    "                     \"Boekingskanaal\": \"Booking Channel\",\n",
    "                     \"Sub-kanaal\": \"Booking Sub-Channel\", \n",
    "                     \"Einddatum segment\": \"Date of Last Segment\", \n",
    "                     \"Nieuw/Wijziging/Annulering\": \"New/Changed/Cancelled\", \n",
    "                     \"Eindbestemming vlucht\": \"Destination\"},inplace = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1311,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Make sure to drop any duplicates\n",
    "df_copy.drop_duplicates(inplace = True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1312,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Convert Departure Date, issuance date to datetime\n",
    "df_copy[\"Departure Date\"] = pd.to_datetime(df_copy[\"Departure Date\"], format=\"%d-%m-%Y\")\n",
    "df_copy[\"Ticket Issuance Date\"] = pd.to_datetime(df_copy[\"Ticket Issuance Date\"], format=\"%d-%m-%Y\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1313,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create where changed or cancelled\n",
    "#Where W = changed and A = Cancelled\n",
    "df_copyChangedCancelled = df_copy[(df_copy[\"New/Changed/Cancelled\"] == \"A\") |  (df_copy[\"New/Changed/Cancelled\"] == \"W\")]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1314,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Drop na\n",
    "df_copyChangedCancelled = df_copyChangedCancelled.dropna(subset=['Airline Code', 'Ticket Issuance Date'])"
   ]
