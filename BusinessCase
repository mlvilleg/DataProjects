    "after_start_date = df_copyChangedCancelled[\"Ticket Issuance Date\"] >= start_date\n",
    "before_end_date = df_copyChangedCancelled[\"Ticket Issuance Date\"] <= end_date\n",
    "between_two_dates = after_start_date & before_end_date\n",
    "filtered_dates = df_copyChangedCancelled.loc[between_two_dates]"
