   "source": [
    "#This is where I make the combined .csv files into one master file\n",
    "df_list = []\n",
    "for file in os.listdir(\"/Users/marckreistian/Desktop/Travix/TM_datafiles\"):\n",
    "    df = pd.read_csv(file,skiprows=[0,1,2,3],sep = \";\") #Skip the first 3 rows as it is not needed\n",
    "    df_list.append(df)\n",
    "print(len(df_list))\n",
    "\n",
    "for i in range(len(df_list)):\n",
    "    df = df.append(df_list[i])\n",
    "len(df)\n",
    "\n",
    "#Do not put the new combined csv in the same folder as the old csv files just in case you need to add more csv files\n",
    "path = \"/Users/marckreistian/Desktop/Travix\"\n",
    "df.to_csv(os.path.join(path,'final.csv'), index=False)\n",
    "df\n",
    " \n"
   ]
